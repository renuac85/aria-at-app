{
  "name": "client",
  "version": "1.0.0",
  "description": "React client for ARIA-AT report",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e ../config/dev.env -- webpack-dev-server --mode development",
    "build": "webpack --config webpack.prod.js",
    "jest": "dotenv -e ../config/test.env -- jest",
    "prettier": "prettier --write \"**/*.{js,jsx,css}\"",
    "lint": "eslint --ext js,jsx .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lighthouse": "lhci autorun"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bocoup/aria-at-report.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/bocoup/aria-at-report/issues"
  },
  "homepage": "https://github.com/bocoup/aria-at-report#readme",
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.21.4",
    "bootstrap": "^4.4.1",
    "core-js": "^3.8.0",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.1",
    "object-hash": "^2.2.0",
    "octicons-react": "^1.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0",
    "react-id-generator": "^3.0.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "showdown": "^1.9.1",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@lhci/cli": "^0.3.12",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^5.1.1",
    "dotenv-webpack": "^1.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-enzyme": "^7.1.2",
    "prettier": "^1.19.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "test": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "globalSetup": "<rootDir>/jest.server.js",
    "globalTeardown": "<rootDir>/jest.teardown.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/resources/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/resources/"
    ]
  }
}
